{
    "name": "timely",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "electron ./dist/main.bundle.js",
        "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
        "dev:main": "cross-env NODE_ENV=development webpack --mode development --watch --progress --color",
        "dev:renderer": "cross-env NODE_ENV=development webpack serve --config=webpack.config.server.js --mode development --progress --color",
        "prod": "cross-env NODE_ENV=production webpack --mode production --progress --color",
        "build:win": "electron-builder build --win --x64",
        "build:mac": "electron-builder build --mac --x64",
        "build": "npm run prod && npm run build:win && npm run build:mac",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/lodash": "^4.14.168",
        "@types/react": "^17.0.1",
        "@types/react-dom": "^17.0.0",
        "@typescript-eslint/eslint-plugin": "^4.14.2",
        "@typescript-eslint/parser": "^4.14.2",
        "concurrently": "^5.3.0",
        "cross-env": "^7.0.3",
        "css-loader": "^5.0.1",
        "electron": "^11.2.2",
        "electron-builder": "^22.9.1",
        "eslint": "^7.19.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.0.0",
        "mini-css-extract-plugin": "^1.3.5",
        "prettier": "^2.2.1",
        "sass": "^1.32.6",
        "sass-loader": "^10.1.1",
        "style-loader": "^2.0.0",
        "ts-loader": "^8.0.14",
        "webpack": "^5.20.1",
        "webpack-cli": "^4.5.0",
        "webpack-dev-server": "^3.11.2"
    },
    "dependencies": {
        "@adobe/react-spectrum": "^3.7.0",
        "@spectrum-icons/workflow": "^3.2.0",
        "clsx": "^1.1.1",
        "formik": "^2.2.6",
